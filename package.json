{
  "name": "all-fours",
  "version": "0.8.0",
  "scripts": {
    "dev": "npm-run-all dev:preflight -p dev:res dev:styles dev:pack dev:server",
    "build": "run-s build:preflight clean:res build:res build:styles build:pack",
    "start": "node src/Server.bs.js",
    "dev:preflight": "run-s build:res build:pack",
    "dev:styles": "postcss src/styles --verbose --dir ./build -w",
    "dev:pack": "webpack -w",
    "dev:server": "nodemon src/Server.bs.js | pino-pretty -t -m '_msg'",
    "dev:res": "rescript build -w -with-deps",
    "build:preflight": "node scripts/check_production_ready.js",
    "clean:res": "rescript clean",
    "build:res": "rescript build -with-deps",
    "build:styles": "postcss src/styles --dir ./build",
    "build:pack": "webpack",
    "update": "./scripts/update.sh",
    "stmux": "stmux --mouse -- [ [ 'echo hello' .. 'npm run dev:styles' ] : [ 'npm run dev:pack' .. 'npm run dev:server' ] ]"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dicebear/avatars-female-sprites": "^4.10.8",
    "@dicebear/avatars-jdenticon-sprites": "^4.10.8",
    "@dicebear/avatars-male-sprites": "^4.10.8",
    "@dicebear/collection": "^7.0.1",
    "@dicebear/core": "^7.0.1",
    "@nireno/reasonably-typed": "^0.18.1",
    "@nireno/rescript-socket.io": "^0.1.5",
    "@rescript/react": "^0.10.3",
    "ansi-colors": "^4.1.1",
    "bs-express": "^1.0.2",
    "clipboard": "^2.0.4",
    "decco": "^1.2.1",
    "dotenv": "^8.1.0",
    "express": "^4.18.2",
    "fs-extra": "^8.1.0",
    "howler": "^2.1.2",
    "nanoid": "^2.1.2",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-spring": "^8.0.27",
    "rescript-express": "^0.4.1",
    "rescript-webapi": "^0.7.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^10.4.13",
    "bs-platform": "^7.3.2",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^3.2.1",
    "postcss": "^8.4.19",
    "rescript": "^10.0.1",
    "tailwindcss": "^3.2.4",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0"
  },
  "volta": {
    "node": "16.18.1"
  }
}
