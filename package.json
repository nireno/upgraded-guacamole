{
  "name": "all-fours",
  "version": "0.1.0",
  "scripts": {
    "bsb": "bsb -make-world -clean-world",
    "bsb:watch": "BS_WATCH_CLEAR=1 bsb -make-world -clean-world -w",
    "bsb:clean": "bsb -clean-world",
    "build": "node scripts/dotenv_diff.js && npm run postcss && npm run bsb && npm run webpack",
    "postcss": "postcss src/styles --dir ./build",
    "postcss:watch": "postcss src/styles --verbose --dir ./build -w",
    "start": "node src/Server.bs.js",
    "start:watch": "nodemon src/Server.bs.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack",
    "webpack:watch": "webpack -w",
    "update": "./scripts/update.sh"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@ryb73/decco": "0.0.14",
    "ansi-colors": "^4.1.1",
    "bs-express": "^0.12.0",
    "bs-socket": "github:nireno/bs-socket.io",
    "bs-webapi": "^0.15.2",
    "dotenv": "^8.0.0",
    "nanoid": "^2.0.3",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-spring": "^8.0.23",
    "reason-react": "^0.7.0",
    "socket.io": "^2.2.0",
    "tailwindcss": "^1.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "bs-platform": "^5.0.4",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "postcss-cli": "^6.1.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.1.8"
  }
