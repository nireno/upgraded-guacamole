{
  "name": "all-fours",
  "version": "0.1.0",
  "scripts": {
    "bsb": "bsb -make-world -clean-world",
    "bsb:watch": "BS_WATCH_CLEAR=1 bsb -make-world -clean-world -w",
    "bsb:clean": "bsb -clean-world",
    "build": "node scripts/check_production_ready.js && npm run postcss && npm run bsb && npm run webpack",
    "postcss": "postcss src/styles --dir ./build",
    "postcss:watch": "postcss src/styles --verbose --dir ./build -w",
    "start": "node src/Server.bs.js",
    "start:watch": "nodemon src/Server.bs.js | pino-pretty -t -m '_msg'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack",
    "webpack:watch": "webpack -w",
    "update": "./scripts/update.sh"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^5.0.1",
    "@nireno/reasonably-typed": "github:nireno/reasonably-typed",
    "@ryb73/decco": "0.0.14",
    "ansi-colors": "^4.1.1",
    "bs-express": "^0.12.1",
    "bs-socket": "github:nireno/bs-socket.io",
    "bs-webapi": "^0.15.4",
    "clipboard": "^2.0.4",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "howler": "^2.1.2",
    "nanoid": "^2.1.2",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-spring": "^8.0.27",
    "reason-react": "^0.7.0",
    "socket.io": "^2.3.0",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "bs-platform": "^5.0.6",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.3",
    "pino-pretty": "^3.2.1",
    "postcss-cli": "^6.1.3",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.8.2"
  }
}
