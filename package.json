{
  "name": "all-fours",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm-run-all dev:preflight -p dev:styles dev:pack dev:server",
    "build": "run-s build:preflight clean:bs build:bs build:styles build:pack",
    "start": "node src/Server.bs.js",
    "dev:preflight": "run-s build:bs build:pack",
    "dev:styles": "postcss src/styles --verbose --dir ./build -w",
    "dev:pack": "webpack -w",
    "dev:server": "nodemon src/Server.bs.js | pino-pretty -t -m '_msg'",
    "dev:bs": "BS_WATCH_CLEAR=1 bsb -make-world -w",
    "build:preflight": "node scripts/check_production_ready.js",
    "clean:bs": "bsb -clean-world",
    "build:bs": "bsb -make-world",
    "build:styles": "postcss src/styles --dir ./build",
    "build:pack": "webpack",
    "update": "./scripts/update.sh",
    "stmux": "stmux --mouse -- [ [ 'echo hello' .. 'npm run dev:styles' ] : [ 'npm run dev:pack' .. 'npm run dev:server' ] ]"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nireno/bs-socketio": "^0.2.5",
    "@nireno/reasonably-typed": "^0.18.1",
    "ansi-colors": "^4.1.1",
    "bs-express": "^1.0.1",
    "bs-webapi": "^0.15.4",
    "clipboard": "^2.0.4",
    "decco": "^1.2.1",
    "dotenv": "^8.1.0",
    "fs-extra": "^8.1.0",
    "howler": "^2.1.2",
    "nanoid": "^2.1.2",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-spring": "^8.0.27",
    "reason-react": "^0.7.0",
    "socket.io": "^2.3.0",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "bs-platform": "^7.3.2",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^3.2.1",
    "postcss-cli": "^7.1.1",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0"
  },
  "volta": {
    "node": "12.16.3"
  }
}
