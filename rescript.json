{
  "name": "all-fours",
  "jsx": {
    "version": 4,
    "mode": "classic"
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "@greenlabs/ppx-spice",
    "@nireno/rescript-socket.io",
    "@rescript/react",
    "rescript-express",
    "rescript-webapi"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-spice/ppx"
  ],
  "uncurried": false
}